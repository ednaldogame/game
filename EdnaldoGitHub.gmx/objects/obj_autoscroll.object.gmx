{
    if instance_exists(obj_fleig_cursor)
    {
        x_speed = scr_approach(x_speed, scroll_xspeed/2, 0.05)
    }
    else if !global.main_player.control_lock
    {
        x_speed = scr_approach(x_speed, scroll_xspeed, 0.0125)
    }
    else
    {
        x_speed = scr_approach(x_speed, 0, 0.05)
    }
    
    x += x_speed;
    
    if obj_camera.cam_obj = global.main_player
    {
        obj_camera.cam_obj = self.id;
        x = view_xview[0] + 512/2;
        y = global.main_player.y;
    }
}

</string>
