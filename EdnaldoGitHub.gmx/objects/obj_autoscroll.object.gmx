            <string>// Keep Player Within Boundaries
with obj_player
{
    if x &lt; view_xview[0] &amp;&amp; !instance_place(view_xview[0], y, par_solid)
    {
        x = view_xview[0];
    }
    
    
    if x &gt; view_xview[0] + 512 &amp;&amp; !instance_place(view_xview[0] + 512, y, par_solid)
    {
        x = view_xview[0] + 512;
    }
}

// Move
