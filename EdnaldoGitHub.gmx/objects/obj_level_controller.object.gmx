    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Handle Faces
// Timers.
    if face_timer[0] &gt; -1
    {
        face_timer[0] --;
    }
    
    if face_timer[1] &gt; -1
    {
        face_timer[1] --;
    }

// Does the Ai Exists?
    ai_exists = instance_exists(obj_ai_player);

// Setup
    var i;
    for (i = 0; i &lt; ai_exists + 2; i += 1)
    {
        var player;
        player = noone;
        
        // Main Player
        if i = 1
        {
            player = global.main_player;
        }  
        
        // AI.
        if i = 2 &amp;&amp; global.cpu_player != noone &amp;&amp; ai_exists
        {
            player = global.cpu_player;
        } 
        
        if player != noone &amp;&amp; instance_exists(player)
        {
            var cur;
            cur = i - 1;
        
            // Ednaldo Pereira
            if (player.player_index = CHAR_EDNALDO)
            {
                // Set Main Frame
                if (head_frame[cur] = -1)
                {
                    head_frame[cur] = 0;
                }
            
                // Normal Frames
                if (player.state = STATE_DEFAULT)
                {
                    if (face_timer[cur] = -1)
                    {
                        if (head_frame[cur] = 0)
                        {
                            head_frame[cur] = 1;
                            face_timer[cur] = 9;
                            exit;
                        }
                        
                        if (head_frame[cur] = 1)
                        {
                            head_frame[cur] = 2;
                            face_timer[cur] = 12;
                            exit;
                        }
                        
                        if (head_frame[cur] = 2)
                        {
                            head_frame[cur] = 3;
                            face_timer[cur] = 12;
                            exit;
                        }
                        
                        if (head_frame[cur] = 3)
                        {
                            head_frame[cur] = 0;
                            face_timer[cur] = irandom_range(60, 210);
                            exit;
                        }
                    }
                }
                
                // Hurt Frames
                if (player.inv_timer &gt; 0 or (instance_exists(obj_hurt_bars) &amp;&amp; !player.ai_flag))
                {
                    if player.image_blend = c_red
                        {
                            head_frame[cur] = 5;
                        }
                        else
                        {
                            head_frame[cur] = 6;
                        }
                    }
                    else
                    {
                        if head_frame[cur] = 6 or head_frame[cur] = 5
                        {
                            head_frame[cur] = 0;
                        }
                }
                
                // Attacking Frames
                if (player.harmful)
                {
                            head_frame[cur] = 4;
                    }
                    else
                    {
                        if head_frame[cur] = 4
                        {
                            head_frame[cur] = 0;
                        }
                }
            }
            
            // Fleig Baixista
            if player.player_index = CHAR_FLEIG
            {
                // Default Frame
                if (head_frame[cur] = -1)
                {
                    head_frame[cur] = 7;
                }    
            
                // Normal Frames
                if (player.state = STATE_DEFAULT &amp;&amp; player.ground)
                {
                    if (face_timer[cur] = -1)
                    {
                        if (head_frame[cur] = 7)
                        {
                            head_frame[cur] = 8;
                            face_timer[cur] = 20;
                            exit;
                        }
                        
                        if (head_frame[cur] = 8)
                        {
                            head_frame[cur] = 9;
                            face_timer[cur] = 12;
                            exit;
                        }
                        
                        if (head_frame[cur] = 9)
                        {
                            head_frame[cur] = 10;
                            face_timer[cur] = 20;
                            exit;
                        }
                        
                        if (head_frame[cur] = 10)
                        {
                            head_frame[cur] = 7;
                            face_timer[cur] = irandom_range(80, 230);
                            exit;
                        }
                    }
                }
                
                // Air
                if (player.state = STATE_DEFAULT &amp;&amp; !player.ground)
                {
                    if player.y_speed &lt; 0
                    {
                        head_frame[cur] = 11;
                    }
                    else
                    {
                        head_frame[cur] = 12;
                    }
                }
                else
                {
                    if head_frame[cur] = 11 or head_frame[cur] = 12
                    {
                        head_frame[cur] = 7;
                    }
                }
                
                
                // Hurt Frames
                if (player.inv_timer &gt; 0 or (instance_exists(obj_hurt_bars) &amp;&amp; !player.ai_flag))
                {
                    if player.image_blend = c_red
                        {
                            head_frame[cur] = 15;
                        }
                        else
                        {
                            head_frame[cur] = 16;
                        }
                    }
                    else
                    {
                        if head_frame[cur] = 15 or head_frame[cur] = 16
                        {
                            head_frame[cur] = 7;
                        }
                }
                
                // Attacking Frames
                if (player.harmful)
                {
                            if(floor(player.animation_frame mod 2) == 1)
                            {
                                head_frame[cur] = 13;
                            }
                            else
                            {
                                head_frame[cur] = 14;
                            }
                    }
                    else
                    {
                        if head_frame[cur] = 13 or head_frame[cur] = 14
                        {
                            head_frame[cur] = 7;
                        }
                }
                
                
            }
        }
    }

</string>
          </argument>
        </arguments>
